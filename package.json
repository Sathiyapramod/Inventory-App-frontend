{
  "name": "my-app",
  "version": "0.1.0",
  "productName": "Electron App Example",
  "main": "public/main.js",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.15",
    "@react-pdf/renderer": "^3.1.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.8",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-to-print": "^2.14.12",
    "recharts": "^2.5.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set BROWSER=none && react-scripts start",
    "start-electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "package": "electron-builder build --mac --win -c.extraMetadata.main=build/main.js --publish never",
    "publish": "electron-builder build --mac --win --publish always",
    "build": "react-scripts build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "build-electron": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "prettier": "^3.4.2"
  },
  "build": {
    "productName": "React Electron App",
    "mac": {
      "publish": {
        "provider": "github",
        "owner": "Sathiyapramod",
        "repo": "capstone-inventory-backend",
        "token": ""
      }
    },
    "win": {
      "publish": {
        "provider": "github",
        "owner": "Sathiyapramod",
        "repo": "capstone-inventory-backend",
        "token": ""
      }
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "owner": "Sathiyapramod"
    }
  }
}
